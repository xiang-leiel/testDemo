<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quantchi.tianji.service.search.dao.MeetingDao">

    <select id="listMeetingInfoByCondition" parameterType="java.util.Map" resultType="java.lang.String">
        SELECT
            img_url
        FROM
            inv_meeting_info
        WHERE
            1 = 1
        <if test="city != null">
            AND city = #{city}
        </if>
        <if test="category != null">
            AND category = #{category}
        </if>
    </select>

</mapper>